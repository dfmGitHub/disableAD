{
  "manifest_version": 2,
  "name": "disableAD",
  // 插件的版本
  "version": "1.0.1",
  // 需要直接注入页面的JS
  "content_scripts":
  [
    {
      "matches": ["<all_urls>"],
      "js": ["js/common.js"]
    },
    {
      //"matches": ["http://*/*", "https://*/*"],
      // "<all_urls>" 表示匹配所有地址
      "matches": ["http://c.biancheng.net/*"],
      // 多个JS按顺序注入
      "js": ["js/cBiancheng.js"]
    },
    {
      "matches": ["https://www.v2ex.com/*"],
      "js": ["js/v2ex.js"]
    },
    {
      "matches": ["https://blog.csdn.net/*"],
      "js": ["js/csdn.js"]
    }
  ]
}
